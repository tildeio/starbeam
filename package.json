{
  "version": "0.0.0",
  "type": "module",
  "types": "src/index.ts",
  "main": "src/index.ts",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "rollup -c ./rollup.config.js",
    "prebuild": "tsc -p ./tsconfig-rollup.json",
    "dev": "esno ./.scripts/scripts.ts",
    "demo": "esno ./.scripts/scripts.ts demo",
    "lint": "eslint --cache -c ./.eslintrc.json --ext .ts,.tsx .",
    "test": "vitest",
    "test:dev": "vitest --mode=development",
    "test:prod": "PROD=1 DEV= vitest",
    "test:types": "tsc --build",
    "test:lint": "eslint . --cache",
    "test:all": "esno ./scripts/scripts.ts test",
    "typecheck": "tsc --build",
    "demos:react:store": "pnpm demo react-store"
  },
  "devDependencies": {
    "@changesets/cli": "^2.24.4",
    "@domtree/any": "workspace:^",
    "@domtree/flavors": "workspace:^",
    "@domtree/minimal": "workspace:^",
    "@eslint/eslintrc": "^1.3.2",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-sucrase": "^4.0.4",
    "@rollup/plugin-typescript": "^8.5.0",
    "@swc/core": "^1.3.1",
    "@swc/helpers": "^0.4.11",
    "@types/node": "^18.7.18",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "@typescript-eslint/scope-manager": "^5.37.0",
    "@typescript-eslint/type-utils": "^5.37.0",
    "@typescript-eslint/types": "^5.37.0",
    "@typescript-eslint/utils": "^5.37.0",
    "@typescript-eslint/visitor-keys": "^5.37.0",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/ui": "^0.23.2",
    "dotenv": "^16.0.2",
    "esbuild": "^0.15.7",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-module-utils": "^2.7.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-rule-composer": "^0.3.0",
    "eslint-scope": "^7.1.1",
    "eslint-utils": "^3.0.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "magic-string": "^0.26.3",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-linter-helpers": "^1.0.0",
    "rollup": "^2.79.0",
    "rollup-plugin-esbuild": "^4.10.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-swc": "^0.2.1",
    "rollup-plugin-ts": "^3.0.2",
    "shelljs": "^0.8.5",
    "typescript": "next",
    "vite": "^3.1.1",
    "vitest": "^0.23.2",
    "watcher": "^1.2.0",
    "preact": "^10.11.0"
  },
  "pnpm": {
    "overrides": {
      "typescript": "next",
      "vite": "^3",
      "vue": "^3.2.37",
      "@types/react": "^18.0.20",
      "@types/react-dom": "^18.0.6",
      "stacktracey": "file:./stacktracey.tgz"
    },
    "peerDependencyRules": {
      "allowAny": [
        "react",
        "react-dom"
      ]
    }
  },
  "nodemonConfig": {
    "watch": [
      "package.json",
      "pnpm-workspace.yaml",
      "scripts"
    ]
  },
  "volta": {
    "node": "18.7.0"
  }
}
